{
  "name": "@pasture-pilot/api",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "ingest-sample": "ts-node src/scripts/ingest-sample-data.ts",
    "kb-stats": "ts-node -e \"import('./src/agents/kb-store').then(({kbStore}) => kbStore.initialize().then(() => kbStore.getStats()).then(console.log))\"",
    "test-kb": "ts-node src/scripts/test-kb-connection.ts",
    "test-kb-debug": "ts-node src/scripts/test-kb-connection.ts --debug",
    "test-kb-types": "ts-node src/scripts/test-kb-types.ts",
    "proof-of-concept": "ts-node src/scripts/proof-of-concept.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.5",
    "openai": "^4.20.1",
    "zod": "^3.22.4",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "uuid": "^9.0.1",
    "date-fns": "^3.0.6",
    "mongodb": "^6.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "prettier": "^3.1.1",
    "@testing-library/jest-dom": "^6.1.6",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11"
  }
}